                  >
                </td>
  
                <td><!-- course details -->
                  <a href="#courseDetails">
                    <font face="arial" color="#ffffff" size="3"> CourseDetails </font></a
                  >
                </td>
  
                <td><!-- project details -->
                  <a href="#ProjectDetails">
                    <font face="arial" color="#ffffff" size="3">
                      ProjectDetails
                    </font></a
                  >
                </td>
  
                <td><!-- team -->
